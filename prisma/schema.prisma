datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Note {
  id        String   @id @default(uuid())
  note      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
